# API Configuration
# Copy this file to .env and fill in your actual API keys

# Autodesk Forge API
FORGE_CLIENT_ID=your_forge_client_id_here
FORGE_CLIENT_SECRET=your_forge_client_secret_here
FORGE_API_KEY=your_forge_api_key_here

# ShareCAD API (Free - no key needed)
SHARECAD_ENABLED=true

# CADViewer360 API
CADVIEWER360_API_KEY=your_cadviewer360_api_key_here
CADVIEWER360_ENABLED=false

# CloudConvert API
CLOUDCONVERT_API_KEY=your_cloudconvert_api_key_here
CLOUDCONVERT_ENABLED=false

# Application Settings
APP_NAME=AutoCAD 3D to 2D AI Processor
APP_VERSION=1.0.0
DEBUG=false

# Server Settings
HOST=0.0.0.0
PORT=8000
WORKERS=1

# File Settings
MAX_FILE_SIZE=104857600  # 100MB in bytes
UPLOAD_DIR=uploads
OUTPUT_DIR=outputs
TEMP_DIR=temp

# AI Settings
AI_CONFIDENCE_THRESHOLD=0.7
BATCH_SIZE=5
MAX_COMPONENTS=1000

# Security Settings
SECRET_KEY=your-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Logging Settings
LOG_LEVEL=INFO
LOG_FILE=logs/app.log

# Cleanup Settings
CLEANUP_INTERVAL_HOURS=24
MAX_FILE_AGE_HOURS=72

# Database Settings (for future use)
DATABASE_URL=sqlite:///./autocad_processor.db

# Redis Settings (for caching)
REDIS_URL=redis://localhost:6379
